#: ../src/nm-sstp-service.c:214
#, c-format
msgid "no usable addresses returned for SSTP VPN gateway '%s' (%d)"
msgstr "nebyla vrácena žádná použitelná adresa brány SSTP VPN „%s“ (%d)"

#: ../src/nm-sstp-service.c:241
msgid "Could not find secrets (connection invalid, no vpn setting)."
msgstr "Nelze nalézt hesla (neplatné připojení, žádná nastavení vpn)."

#: ../src/nm-sstp-service.c:253
msgid "Invalid VPN username."
msgstr "Neplatné jméno uživatele VPN."

#: ../src/nm-sstp-service.c:262
msgid "Missing VPN username."
msgstr "Chybí jméno uživatele VPN."

#: ../src/nm-sstp-service.c:272
msgid "Missing or invalid VPN password."
msgstr "Chybějící nebo neplatné heslo VPN."

#: ../src/nm-sstp-service.c:430
msgid "No cached credentials."
msgstr "Žádné přihlašovací údaje v mezipaměti."

#: ../src/nm-sstp-service.c:577
#, c-format
